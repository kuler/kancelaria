<% if @count > 0 %>    <h3><%=t('search_results_for')%>: <%=@query %></h3>    <% @results.each_key do |k|%>        <dl class='dl-horizontal'>                <% if ! @results[k].empty? %>            <% @results[k].each do |v| %>                <%= render :partial => k.to_s, :locals=>{:object=>v} %>            <% end %>            <% end %>        </dl>    <% end %><% else %>    <h3><%=t('search_not_found')%> <%=link_to "Google", 'http://google.com/search?q='+@query, :target=>'_blank'%></h3><% end %>